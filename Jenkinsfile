pipeline {
    agent any 
    
    stages {
        stage('Build') {
            steps {
                sh 'echo "Building feature branch..."'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Testing feature branch..."'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploying feature branch..."'
            }
        }
    }
}