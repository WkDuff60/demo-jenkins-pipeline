pipeline {
    agent any 
    
    stages {
        stage('Build') {
            steps {
                sh 'echo "Building master branch..."'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Testing master branch..."'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploying master branch..."'
            }
        }
    }
}